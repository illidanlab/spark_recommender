<jobList xmlns:recsys="Samsung Recsys">
	<jobEntry>
		<jobType>recommendation</jobType>
		<jobName>exp_test_20140522_20140525</jobName>
		<jobDesc>Test experiment from 20140522 to 20140525</jobDesc>
		<sparkContext>
			<master>local[4]</master>
		</sparkContext>
		<resourceLocation>
			<roviHq>data/ROVI/</roviHq>
			<watchTime>data/ACR/</watchTime>
			<workspace>/tmp/vddil/recsys</workspace>
			<!--<roviHq>hdfs://gnosis-01-01-01.crl.samsung.com/apps/vddil/rovi_hq</roviHq>
			<watchTime>hdfs://gnosis-01-01-01.crl.samsung.com/apps/vddil/duid-program-watchTime</watchTime>
			<workspace>hdfs://gnosis-01-01-01.crl.samsung.com/apps/data/vddil/recsys</workspace>-->
		</resourceLocation>
		<trainDates>
			<date>20140522</date>
			<date>20140523</date>
			<date>20140524</date>
			<date>20140525</date></trainDates>
		<features>			
			<feature>
				<type>itemFeature</type>
				<name>syn_topic</name>
				<param>
					<topicType>wiki</topicType>
					<topicDim>200</topicDim>
				</param>
			</feature>
			<feature>
				<type>itemFeature</type>
				<name>rand</name>
				<param>
					<dim>4</dim>
				</param>
			</feature>
			<feature>
				<type>userFeature</type>
				<name>zap</name>
				<param>
					<interval>2</interval>
				</param>
			</feature>
			<feature>
				<type>userFeature</type>
				<name>watchtime</name>
				<param>
					<minThr>10</minThr>
				</param>
			</feature>
			<feature>
				<type>factFeature</type>
				<name>pmf</name>
				<param>
					<dim>10</dim>
				</param>
			</feature>
		</features>
		<dataSplit>
			<testingRatio>0.2</testingRatio>
			<validationRatio>0.1</validationRatio>
		</dataSplit>
		<models>
			<model>
				<type>score_reg</type>
				<name>ridge_reg</name>
				<param>
					<rho>1.5</rho>
				</param>
			</model>
			<model>
				<type>score_reg</type>
				<name>ridge_reg</name>
				<param>
					<rho>0.1</rho>
				</param>
			</model>
		</models>
		<ensembles>
			<ensemble>
				<type>max</type>
				<param>
					<num>2</num>
				</param>
			</ensemble>
		</ensembles>
	</jobEntry>
	<jobEntry>
		<jobType>helloworld</jobType>
		<jobName>simple_spark_test</jobName></jobEntry></jobList>