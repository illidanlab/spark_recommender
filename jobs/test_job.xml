<jobList xmlns:recsys="Samsung Recsys">
	<jobEntry>
		<jobType>recommendation</jobType>
		<jobName>exp_test_20140522_20140525</jobName>
		<jobDesc>Test experiment from 20140522 to 20140525</jobDesc>
		<sparkContext>
			<master>local[4]</master>
		</sparkContext>
		<resourceLocation>
			<roviHq>data/ROVI/</roviHq>
			<watchTime>data/ACR/</watchTime>
			<workspace>/tmp/vddil/recsys</workspace>
		</resourceLocation>
		<trainDates>
			<date>20140522</date>
			<date>20140523</date>
			<date>20140524</date>
			<date>20140525</date></trainDates>
		<features>		
			<feature>
				<type>itemFeature</type>
				<name>genre</name>
				<param>
					<lang>en</lang>
				</param>
			</feature>	
			<feature>
				<type>userFeature</type>
				<name>genre</name>
			</feature>
		</features>
		<dataSplit>
			<testingRatio>0.2</testingRatio>
			<validationRatio>0.1</validationRatio>
		</dataSplit>
		<models>
			<model>
				<type>score_reg</type>
				<name>ridge_reg</name>
				<param>
					<regParam>1.5</regParam>
					<minUFCoverage>0.8</minUFCoverage>
					<minIFCoverage>0.8</minIFCoverage>
					<numIterations>100</numIterations>
					<stepSize>1</stepSize>
				</param>
			</model>
			<model>
				<type>score_reg</type>
				<name>ridge_reg</name>
				<param>
					<regParam>0.1</regParam>
					<numIterations>100</numIterations>
					<stepSize>1</stepSize>
					<minUFCoverage>0.7</minUFCoverage>
					<minIFCoverage>0.7</minIFCoverage>
				</param>
			</model>
		</models>
		<ensembles>
			<ensemble>
				<type>max</type>
				<param>
					<num>2</num>
				</param>
			</ensemble>
		</ensembles>
	</jobEntry>
